{
  "name": "poker-results-tracker-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test:e2e": "start-server-and-test 'npm run dev' http://localhost:3001 'cypress open --e2e'",
    "test:e2e:login": "start-server-and-test 'npm run dev' http://localhost:3001 'cypress run --spec \"cypress/e2e/login.cy.ts\"'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:component": "cypress run --component"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "dotenv": "^16.4.7",
    "pinia": "^2.2.4",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.4"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.2",
    "@cypress/vue": "^6.0.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/eslint-plugin": "1.1.7",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^14.2.0",
    "cypress-vite": "^1.6.0",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.6.0",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.5.3",
    "vitest": "^2.1.3",
    "vue-tsc": "^2.1.6"
  }
}
